    <div class="mt-6">
      <time value="{{ post.date }}" class="text-gray-600">{{ post.date | readableDate }}</time>
      <h4 class="text-lg font-bold"><a href="{{ post.url }}">{{ post.data.title }}</a></h4>
      <p class="text-sm text-gray-600">{{ post.data.tags | tagList | safe }}</p>
    </div>
